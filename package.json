{
  "name": "tablab",
  "version": "1.0.0-alpha.0",
  "description": "A javascript library to write tablatures",
  "keywords": [
    "tabs",
    "guitar tabs",
    "tablature",
    "guitar",
    "parser"
  ],
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "rollup -c rollup.config.js",
    "postbuild": "rimraf lib/**/",
    "build:clean": "rimraf lib",
    "predocs": "npm run docs:clean",
    "docs": "npx typedoc --out docs --name \"Tablab\" --readme \"none\" --includeVersion --excludePrivate src/index.ts",
    "docs:clean": "rimraf docs",
    "test": "jest --no-cache",
    "pretest:coverage": "npm run test:clean",
    "test:coverage": "jest --no-cache --coverage",
    "test:clean": "rimraf coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "release": "standard-version --no-verify",
    "release:dry-run": "npm run release -- --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphael-jorge/tablab.git"
  },
  "author": {
    "name": "Raphael Jorge",
    "url": "https://github.com/raphael-jorge"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raphael-jorge/tablab/issues"
  },
  "homepage": "https://github.com/raphael-jorge/tablab#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.2",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run test"
    }
  }
}
